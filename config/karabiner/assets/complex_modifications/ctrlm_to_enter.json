{
  "title": "ctrl-m_to_enter",
  "rules": [
    {
      "description": "Ctrl-M を Enter に変換",
      "manipulators": [
          {
              "from": {
                  "key_code": "m",
                  "modifiers": {
                      "mandatory": ["control"],
                      "optional": ["left_shift", "left_option", "left_command"]
                  }
              },
              "to": [{ "key_code": "return_or_enter" }],
              "type": "basic"
          }
      ]
    }
  ]
}
